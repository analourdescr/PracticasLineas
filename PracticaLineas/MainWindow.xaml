<Window x:Class="PracticaLineas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaLineas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="AliceBlue">
        <Polyline Name="plnLinea" StrokeThickness="180" Stroke="DarkOrange"/>
        <Polyline Name="plnLinea2" StrokeThickness="80" Stroke="Brown"/>
        <Polyline Name="plnLinea3" StrokeThickness="70" Stroke="DarkGreen"/>
        <Polyline Name="plnLinea4" StrokeThickness="4" Stroke="Black"/>
        <Polyline Name="plnLinea5" StrokeThickness="4" Stroke="Black"/>
        <Polyline Name="plnLinea6" StrokeThickness="3" Stroke="Brown"/>
        <Polyline Name="plnLinea7" StrokeThickness="80" Stroke="Gray"/>
        <Polyline Name="plnLinea8" StrokeThickness="80" Stroke="GreenYellow"/>
    </Grid>
</Window>
